<!DOCTYPE html>
<html lang="pt-BR">
  <!-- Mirrored from coderthemes.com/wb/studyzen/admin-dashboard.html by HTTrack Website Copier/3.x [XR&CO'2014], Wed, 22 Jan 2025 13:08:15 GMT -->
  <head>
    <meta charset="utf-8" />
    <title>Admin - professor</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta content="" name="description" />
    <meta content="coderthemes" name="author" />

    <!-- Theme favicon -->
    <link rel="shortcut icon" href="assets/favicon-8fd74f43.ico" />

    <script
      type="module"
      crossorigin
      src="assets/admin-dashboard-a08afcd4.js"
    ></script>
    <link rel="modulepreload" crossorigin href="assets/theme-e4584917.js" />
    <link
      rel="modulepreload"
      crossorigin
      href="assets/apexcharts.common-ac06378e.js"
    />
    <link rel="stylesheet" href="assets/theme-d32f8eb8.css" />

    <!-- SweetAlert CSS e JS -->
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
  </head>

  <body>
    <!-- Start Sidebar -->
    <div
      id="application-sidebar"
      class="hs-overlay hs-overlay-open:translate-x-0 -translate-x-full transition-all duration-300 transform hidden fixed inset-y-0 start-0 lg:z-30 z-60 w-64 bg-white border-e border-default-200 overflow-y-auto lg:block lg:translate-x-0 lg:end-auto lg:bottom-0 dark:bg-default-50 rtl:hs-overlay-open:translate-x-0 rtl:translate-x-full rtl:lg:translate-x-0 print:hidden"
    >
      <div
        class="flex sticky top-0 items-center justify-center px-6 h-18 border-b border-dashed border-default-200"
      >
        <a href="admin-dashboard.html">
          <img src="assets/saep.png" alt="logo" class="h-9 flex dark:hidden" />
          <img
            src="assets/logo-light-43c180d2.png"
            alt="logo"
            class="h-9 hidden dark:flex"
          />
        </a>
      </div>

      <div class="h-[calc(100%-72px)] p-4" data-simplebar>
        <ul class="admin-menu w-full flex flex-col gap-1.5">
          <li class="menu-item">
            <a
              class="flex items-center gap-x-3.5 py-3 px-5 text-sm font-medium text-default-700 rounded-md transition-all hover:bg-default-100"
              href="/dashboard"
            >
              <i class="ph-duotone ph-gauge text-xl"></i>
              Dashboard
            </a>
          </li>

          <li class="menu-item">
            <a
              class="flex items-center gap-x-3.5 py-3 px-5 text-sm font-medium text-default-700 rounded-md transition-all hover:bg-default-100"
              href="/cadastrar-questao"
            >
              <i class="ph-duotone ph-book text-xl"></i>
              Cadastrar questões
            </a>
          </li>

          <li class="menu-item">
            <a
              class="flex items-center gap-x-3.5 py-3 px-5 text-sm font-medium text-default-700 rounded-md transition-all hover:bg-default-100"
              href="admin-calender.html"
            >
              <i class="ph-duotone ph-calendar text-xl"></i>
              Simulados aplicados
            </a>
          </li>

          <li class="menu-item">
            <a
              class="flex items-center gap-x-3.5 py-3 px-5 text-sm font-medium text-default-700 rounded-md transition-all hover:bg-default-100"
              href="admin-calender.html"
            >
              <i class="ph-duotone ph-database text-xl"></i>
              Banco de questões
            </a>
          </li>
          <li class="menu-item">
            <a
              class="flex items-center gap-x-3.5 py-3 px-5 text-sm font-medium text-default-700 rounded-md transition-all hover:bg-default-100"
              href="/cadastrar-materia"
            >
              <i class="ph-duotone ph-gauge text-xl"></i>
              Cadastrar matéria
            </a>
          </li>
        </ul>
      </div>
    </div>
    <!-- End Sidebar -->
    <!-- Start Content -->
    <div class="min-h-screen flex flex-col lg:ps-64 w-full">
      <!-- Start Header -->
      <header
        class="sticky top-0 h-18 flex z-40 w-full border-b border-default-200/80 bg-white/80 backdrop-blur-3xl dark:bg-default-50 print:hidden"
      >
        <nav class="flex items-center gap-4 w-full px-6">
          <!-- Navigation Toggle (in Small Screen) -->
          <div class="lg:hidden flex">
            <button
              type="button"
              class="text-default-500 hover:text-default-600"
              data-hs-overlay="#application-sidebar"
              aria-controls="application-sidebar"
              aria-label="Toggle navigation"
            >
              <i data-lucide="align-justify" class="w-6 h-6"></i>
            </button>
          </div>

          <!-- Logo -->
          <div class="lg:hidden flex">
            <a href="admin-dashboard.html">
              <img
                src="assets/saep.png"
                alt="logo"
                class="h-10 w-full flex dark:hidden"
              />
              <img
                src="assets/logo-light-43c180d2.png"
                alt="logo"
                class="h-10 w-full hidden dark:flex"
              />
            </a>
          </div>

          <!-- Topbar Link and Dropdown Button -->
          <div class="flex items-center gap-4 ms-auto">
            <!-- Fullscreen Button -->
            <div class="lg:flex hidden">
              <button
                data-toggle="fullscreen"
                class="inline-flex flex-shrink-0 justify-center items-center gap-2 h-10 w-10 rounded-md font-medium text-default-700 align-middle bg-default-100 hover:text-primary transition-all text-xs"
              >
                <i class="ph-duotone ph-arrows-out text-2xl"></i>
              </button>
            </div>

            <!-- Notification Dropdown -->

            <!-- Profile Dropdown -->
            <div class="flex">
              <div class="hs-dropdown relative inline-flex">
                <button
                  id="hs-dropdown-with-header"
                  type="button"
                  class="hs-dropdown-toggle inline-flex justify-center items-center gap-2 flex-shrink-0 rounded-md font-medium text-default-700 align-middle transition-all text-xs"
                >
                  <img
                    class="inline-block h-10 w-10 rounded-full"
                    src="assets/avatar-7-37b455da.png"
                  />
                  <div class="lg:block text-start hidden">
                    <p class="text-xs font-semibold text-default-700">
                      Wanderson Maikon
                    </p>
                    <p class="text-xs text-default-500 mt-1">Professor</p>
                  </div>
                </button>

                <div
                  class="hs-dropdown-menu mt-2 transition-[opacity,margin] duration hs-dropdown-open:opacity-100 opacity-0 hidden min-w-[12rem] shadow-md rounded-lg p-2 border border-default-200 bg-white dark:bg-default-50"
                >
                  <a
                    class="flex items-center gap-x-3.5 py-2 px-3 rounded-md text-sm font-medium text-default-800 transition-all hover:bg-default-100"
                    href="admin-student-detail.html"
                  >
                    <i class="ph-duotone ph-user text-base"></i>
                    Perfil do professor
                  </a>

                  <hr class="my-2 border-default-200 -mx-2" />

                  <a
                    class="flex items-center gap-x-3.5 py-2 px-3 rounded-md text-sm font-medium text-red-400 hover:bg-red-400/10"
                    href="/login"
                  >
                    <i class="w-4 h-4" data-lucide="log-out"></i>
                    Sair
                  </a>
                </div>
              </div>
            </div>
          </div>
        </nav>
      </header>
      <!-- End Header -->

      <footer
        class="p-6 border-t border-default-200 w-full bg-white dark:bg-default-50 mt-auto print:hidden"
      >
        <div class="flex flex-wrap items-center justify-between gap-6">
          <p class="text-default-600 font-semibold lg:text-start text-center">
            <script>
              document.write(new Date().getFullYear());
            </script>
            © SENAI. Todos os direitos reservados.
          </p>
        </div>
      </footer>

      <div
        class="fixed bottom-5 end-5 flex flex-col items-center bg-primary/25 rounded-full z-10 print:hidden"
      >
        <button
          class="h-0 w-8 opacity-0 flex justify-center items-center transition-all duration-500 translate-y-5 z-10"
          data-toggle="back-to-top"
        >
          <i class="h-5 w-5 text-primary-500 mt-1" data-lucide="chevron-up"></i>
        </button>
        <button
          class="rounded-full h-10 w-10 bg-primary text-white flex justify-center items-center z-20"
        >
          <i class="h-5 w-5" data-lucide="sun" id="light-theme"></i>
          <i class="h-5 w-5" data-lucide="moon" id="dark-theme"></i>
        </button>
      </div>
    </div>
    <!-- End Content -->

    <!-- Theme Js -->
  </body>
  <% if (message) { %>
  <script>
    document.addEventListener("DOMContentLoaded", function () {
      Swal.fire({
        title: "Sucesso!",
        text: "<%= message %>",
        icon: "success",
        confirmButtonText: "OK",
        timer: 3000, // O alerta desaparecerá após 3 segundos
        timerProgressBar: true,
      });
    });
  </script>
  <% } %>
